            "keys": ["N1","N2","N3","N4","N5","Q","W","E","R","A","S","D","F","Y","X","C","V","WINDOWS_LEFT","ALT_LEFT","STRG_LEFT"]
        },
        {
            "alias": "Layer2", "type": "SetContinuousColor", "delay": 200000, "interval": 150 ,"color": "ff0000ff", "layer": 2,
            "keys": ["N1","N2","N3","N4","N5","Q","W","E","R","A","S","D","F","Y","X","C","V"]
        },
        {
            "alias": "Layer3", "type": "SetContinuousColor", "delay": 200000, "interval": 270 ,"color": "00ff00ff", "layer": 3,
            "keys": ["N1","N2","N3","N4","N5","Q","W","E","R","A","S","D","F"]
        },
        {
            "alias": "Layer4", "type": "SetContinuousColor", "delay": 200000, "interval": 540 ,"color": "0000ffff", "layer": 4,
            "keys": ["N1","N2","N3","N4","N5","Q","W","E","R"]
        },
        {
            "alias": "Layer5", "type": "SetContinuousColor", "delay": 200000, "interval": 870 ,"color": "ffff00ff", "layer": 5,
